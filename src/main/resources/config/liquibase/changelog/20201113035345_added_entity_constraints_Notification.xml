<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Notification.
    -->
    <changeSet id="20201113035345-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="transmitter_id"
                                 baseTableName="notification"
                                 constraintName="fk_notification_transmitter_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_account"/>

        <addForeignKeyConstraint baseColumnNames="receptor_id"
                                 baseTableName="notification"
                                 constraintName="fk_notification_receptor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_account"/>

    </changeSet>
</databaseChangeLog>
